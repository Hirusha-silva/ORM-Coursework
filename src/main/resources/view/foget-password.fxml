<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="434.0" prefWidth="359.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.project.mentalhealthterapycenter.controller.FogetPassword">
   <children>
      <Label layoutX="90.0" layoutY="20.0" text="Change Password ?" textFill="WHITE">
         <font>
            <Font name="Impact" size="23.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="121.0" text="User Name" textFill="WHITE">
         <font>
            <Font name="Georgia" size="19.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="162.0" text="E-Mail" textFill="WHITE">
         <font>
            <Font name="Georgia" size="19.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="205.0" text="New Password" textFill="WHITE">
         <font>
            <Font name="Georgia" size="19.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="245.0" prefHeight="34.0" prefWidth="160.0" text="Confirm" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Georgia" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="userName" layoutX="174.0" layoutY="119.0" prefHeight="26.0" prefWidth="174.0">
         <font>
            <Font name="Consolas" size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="Email" layoutX="174.0" layoutY="160.0" prefHeight="26.0" prefWidth="174.0">
         <font>
            <Font name="Consolas" size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordFieldOne" layoutX="174.0" layoutY="203.0" prefHeight="26.0" prefWidth="174.0" />
      <PasswordField fx:id="passwordFieldTwo" layoutX="174.0" layoutY="249.0" prefHeight="26.0" prefWidth="174.0" />
      <CheckBox fx:id="showPassWord" layoutX="22.0" layoutY="313.0" mnemonicParsing="false" onAction="#showPassWordAction" text="Show Password" textFill="WHITE">
         <font>
            <Font name="Gadugi" size="12.0" />
         </font>
      </CheckBox>
      <Button fx:id="change" layoutX="205.0" layoutY="355.0" mnemonicParsing="false" onAction="#changeAction" prefHeight="34.0" prefWidth="114.0" style="-fx-background-color: #4834d4; -fx-background-radius: 12;" text="Change" textFill="WHITE">
         <font>
            <Font size="23.0" />
         </font>
      </Button>
